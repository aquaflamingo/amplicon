<% provide(:title, @title) %>
<div class="row">
  <%= render partial: "user_info_aside", object: @user %>

  <div class="col-md-8 ms-3">
    <h3><%= @title %></h3>
    <% if @users.any? %>
      <%= render partial: "/users/list", object: @users %>
    <% end %>

    <%= link_to "Go Back", url_for(:back) %>
  </div>
</div>
