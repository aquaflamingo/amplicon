<!-- Col 3 -->
<h2>@<%= @user.username %></h2>
<p> User Info </p>

<!-- Col 9 -->
<h3> Protocols </h3>
<div>
  <% @user.protocols.each do |p| %>
    <! -- card -->
    <div>
      <%= link_to protocol_path(p) do %>
        <p><%= p.name %></p>
        <p><%= p.description %></p>
        <p><%= p.favorited_by.count %> favorite </p> 
      <% end %>
    </div>
  <% end%>
</div>
